{
  "compilerOptions": {
    "jsx": "react",
    "skipLibCheck": true,
    "emitDeclarationOnly": false,
    "declaration": true,
    "esModuleInterop": true,
    "target": "es2022",
    "lib": ["es2022", "DOM"],
    "sourceMap": true,
    "isolatedModules": true,
    "allowJs": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}
